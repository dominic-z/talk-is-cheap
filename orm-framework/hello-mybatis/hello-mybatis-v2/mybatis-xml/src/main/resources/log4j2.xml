<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%-5level] [%d{HH:mm:ss.SSS}] [%F:%L)]  - %m%n" />
        </Console>
    </Appenders>
    <Loggers>
<!--        控制mybatis的log等级-->
<!--        <Root level="DEBUG">-->
        <Root level="INFO">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuration>